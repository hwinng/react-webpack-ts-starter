{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": false,
      "module": "ESNext",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "strict": true,
      "sourceMap": true,
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "Node",
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"],
        "@@/*": ["./*"]
      },
    },
    "include": ["src/**/*"]
}